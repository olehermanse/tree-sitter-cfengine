{
  "name": "tree-sitter-cfengine",
  "version": "1.0.0",
  "description": "Tree sitter grammar for editor support / syntax highlighting of CFEngine policy files",
  "author": "",
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "generate": "tree-sitter generate",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "parse": "tree-sitter parse example-file"
  },
  "tree-sitter": [
    {
      "scope": "source.cfengine",
      "file-types": [
        "cf"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "github.com/olehermanse/tree-sitter-cfengine"
  },
  "dependencies": {
    "node-addon-api": "^7.1.1",
    "node-gyp-build": "^4.8.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.3"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
